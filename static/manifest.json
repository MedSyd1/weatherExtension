{
  "name": "weather_extension",
  "version" : "1.0.0",
  "description": "just a weather extension",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/16.png",
      "48": "/icons/48.png",
      "128": "/icons/128.png"
    }
  },
  "icons": {
    "16": "/icons/16.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "permissions": ["geolocation"],
  "manifest_version": 3
}
